{
  "name": "nascom/teamleader-api-client",
  "description": "PHP client to connect to the Teamleader API ",
  "type": "library",
  "authors": [{
    "name": "Stijn De Meyere",
    "role": "Lead Developer"
  },
  {
    "name": "Stephan Bisschop",
    "role": "Collaborator"
  },
  {
    "name": "Mark Gerarts",
    "role": "Collaborator"
  }],
  "keywords": [
    "Teamleader",
    "api"
